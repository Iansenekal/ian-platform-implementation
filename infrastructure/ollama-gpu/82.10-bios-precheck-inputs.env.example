# 82.10 BIOS/UEFI + GPU precheck inputs
MOTHERBOARD_MODEL=TUF_Gaming_B560M_PLUS
BIOS_VERSION=0820
CPU_MODEL=Intel_i5_11400
GPU_MODEL=AMD_RX_7900_XT
RAM_SIZE_GB=32
RAM_PROFILE=DDR4_3600
PSU_WATTAGE=750

VT_X_ENABLED=true
VT_D_OR_IOMMU_ENABLED=true
ABOVE_4G_DECODING_ENABLED=true
REBAR_ENABLED=true
PCIE_GEN_POLICY=auto
SECURE_BOOT_POLICY=A
SECURE_BOOT_ENABLED=false

# Planned post-install behavior
REBAR_FALLBACK_DISABLE_IF_UNSTABLE=true
PCIE_GEN_LOCK_FALLBACK=gen4
